    <fragment
        android:id="@+id/navigation_personal"
        android:name="vn.unlimit.vpngate.fragment.paidserver.PersonalFragment"
        android:label="@string/title_personal"
        tools:layout="@layout/fragment_personal" />

    <fragment
        android:id="@+id/navigation_notification_setting"
        android:name="vn.unlimit.vpngate.fragment.paidserver.NotificationSettingFragment"
        android:label="@string/personal_notification_setting"
        tools:layout="@layout/fragment_notification_setting" />

    <fragment
        android:id="@+id/navigation_change_pass"
        android:name="vn.unlimit.vpngate.fragment.paidserver.ChangePassFragment"
        android:label="@string/personal_change_password"
        tools:layout="@layout/fragment_change_pass" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="vn.unlimit.vpngate.fragment.paidserver.ProfileFragment"
        android:label="@string/personal_profile"
        tools:layout="@layout/fragment_profile" />
